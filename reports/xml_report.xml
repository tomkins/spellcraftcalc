<?xml version="1.0" encoding="UTF-8"?>
<ConfigReport>
<center><b>Config Report</b></center><br />
<center><b>Stats</b></center><hr />
<table cellspacing="10" cellpadding="0">
<tr>
	<td>Str:</td>
	<td><charattr attr="Strength" width="3" /></td>
	<td>Int:</td>
	<td><charattr attr="Intelligence" width="3" /></td>
	<td>Hits:</td>
	<td><charattr attr="Hits" width="4" /></td>
</tr>
<tr>
	<td>Con:</td>
	<td><charattr attr="Constitution" width="3" /></td>
	<td>Pie:</td>
	<td><charattr attr="Piety" width="3" /></td>
	<td>Power:</td>
	<td><charattr attr="Power" width="3" /></td>
</tr>
<tr>
	<td>Dex:</td>
	<td><charattr attr="Dexterity" width="3" /></td>
	<td>Cha:</td>
	<td><charattr attr="Charisma" width="3" /></td>
</tr>
<tr>
	<td>Qui:</td>
	<td><charattr attr="Quickness" width="3" /></td>
	<td>Emp:</td>
	<td><charattr attr="Empathy" width="3" /></td>
</tr>
</table><br />
<center><b>Resists</b></center><hr />
<table cellspacing="10" cellpadding="0">
<tr>
	<td>Body:</td>
	<td><charattr attr="Body" width="2" /></td>
	<td>Energy:</td>
	<td><charattr attr="Energy" width="2" /></td>
	<td>Crush:</td>
	<td><charattr attr="Crush" width="3" /></td>
</tr>
<tr>
	<td>Cold:</td>
	<td><charattr attr="Cold" width="2" /></td>
	<td>Matter:</td>
	<td><charattr attr="Matter" width="2" /></td>
	<td>Thrust:</td>
	<td><charattr attr="Thrust" width="2" /></td>
</tr>
<tr>
	<td>Heat:</td>
	<td><charattr attr="Heat" width="2" /></td>
	<td>Spirit:</td>
	<td><charattr attr="Spirit" width="2" /></td>
	<td>Slash:</td>
	<td><charattr attr="Slash" width="3" /></td>
</tr>
</table><br />
<center><b>Skills</b></center><hr />
<charattr attr="skills" /><br />
<center><b>Focus</b></center><hr />
<charattr attr="foci" /><br />
<center><b>Cap Increases</b></center><hr />
<charattr attr="capbonuses" /><br />
<center><b>Other Bonuses</b></center><hr />
<charattr attr="otherbonuses" /><br />
<center><b>Piece Listing</b></center><hr />
<dl>
<?for item in ['Chest','Arms','Head','Legs','Hands','Feet','Right Hand','Left Hand','2 Handed','Ranged']?>
<item>
<dt><b><?item?></b></dt>
<?if itemtype=="player"?>
<dd>Imbue Points: <itemattr attr="usedpoints" /> of <itemattr attr="availablepoints" /> Quality: <itemattr attr="quality" /> Overcharge: <itemattr attr="overcharge" /></dd>
<?for slot in range(1,5)?>
<dd>Gem <?slot?>: <slotattr attr="amount" width="2"/> <slotattr attr="effect"/> - <slotattr attr="quality"/> <slotattr attr="name"/></dd>
<?endfor?>
<?for slot in range(5,7)?>
<dd>Effect <?slot?>: <slotattr attr="amount" width="2"/> <slotattr attr="effect"/> - <slotattr attr="name"/></dd>
<dd>Utility: <itemattr attr="utility"/></dd>
<?endif?>
<?if itemtype=="drop"?>
<dd>Name: <itemattr attr="name"/></dd>
<dd>Level: <itemattr attr="level"/>   Quality: <itemattr attr="quality"/>   AF: <itemattr attr="af"/>   Bonus: <itemattr attr="bonus"/></dd>
<?for slot in range(1,13)?>
<dd>Effect <?slot?>: <slotattr attr="amount" width="3"/> <slotattr attr="effect"/></dd>
<?endfor?>
<dd>Utility: <itemattr attr="utility"/></dd>
<?endif?>
</item>
<?endfor?>
<?for item in ['Neck','Cloak','Jewel','Belt','Right Ring','Left Ring','Right Wrist','Left Wrist']?>
<item>
<dt><b><?item?></b></dt>
<dd>Name: <itemattr attr="name"/></dd>
<dd>Level: <itemattr attr="level"/>   Quality: <itemattr attr="quality"/>   Bonus: <itemattr attr="bonus"/></dd>
<?for slot in range(1,13)?>
<dd>Effect <?slot?>: <slotattr attr="amount" width="3"/> <slotattr attr="effect"/></dd>
<?endfor?>
<dd>Utility: <itemattr attr="utility"/></dd>
</item>
</dl>
</ConfigReport>
